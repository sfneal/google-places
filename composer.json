{
    "name": "sfneal/google-places",
    "description": "Actions & Controllers for interacting with the Google Places API through Laravel applications",
    "keywords": [
        "sfneal",
        "google-places"
    ],
    "homepage": "https://github.com/sfneal/google-places",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Stephen Neal",
            "email": "stephen.neal14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-curl": "*",
        "ext-json": "*",
        "sfneal/actions": ">=0.3.0",
        "sfneal/controllers": ">=0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sfneal\\GooglePlaces\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sfneal\\GooglePlaces\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
